      <Button fx:id="juiceButton" blendMode="HARD_LIGHT" layoutX="46.0" layoutY="146.0" mnemonicParsing="false" onAction="#juicePageFun" prefHeight="235.0" prefWidth="398.0" style="-fx-background-radius: 50;" text="Drinks" textFill="#260e40">
