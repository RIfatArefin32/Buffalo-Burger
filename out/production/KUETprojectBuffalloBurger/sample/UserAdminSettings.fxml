<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="701.0" prefWidth="1000.0" styleClass="userAdminPage" stylesheets="@../Style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.UserAdminSettingsController">
   <children>
      <ImageView fitHeight="107.0" fitWidth="192.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/Logo.png" />
         </image>
         <effect>
            <DropShadow spread="0.68" />
         </effect>
      </ImageView>
      <ImageView fitHeight="101.0" fitWidth="100.0" layoutX="906.0" layoutY="17.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/admin.jpg" />
         </image>
         <effect>
            <DropShadow spread="0.81" />
         </effect>
      </ImageView>
      <Button fx:id="home" layoutX="906.0" layoutY="628.0" mnemonicParsing="false" onAction="#AdminHomeFun" prefHeight="50.0" prefWidth="50.0" style="-fx-background-radius: 100; -fx-background-color: white;" styleClass="back" stylesheets="@../buttonStyle.css" underline="true">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="19.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <DropShadow blurType="GAUSSIAN" spread="0.91">
               <color>
                  <Color red="0.15320108830928802" green="0.4871794879436493" blue="0.16433370113372803" />
               </color></DropShadow>
         </effect>
      </Button>
      <Label layoutX="314.0" layoutY="32.0" text="Set Price and Availability" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="35.0" />
         </font>
         <effect>
            <DropShadow spread="0.47" />
         </effect>
      </Label>
      <Label layoutX="819.0" layoutY="637.0" text="Back" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="27.0" />
         </font>
         <effect>
            <DropShadow spread="0.25" />
         </effect>
      </Label>
      <HBox layoutX="91.0" layoutY="249.0" prefHeight="202.0" prefWidth="840.0" spacing="70.0">
         <children>
            <Button fx:id="burger" mnemonicParsing="false" onAction="#BurgerFun" prefHeight="203.0" prefWidth="236.0" style="-fx-background-radius: 10;" styleClass="burgicon" stylesheets="@../buttonStyle.css">
               <effect>
                  <DropShadow spread="0.85" />
               </effect>
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="49.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="drink" mnemonicParsing="false" onAction="#DrinksFun" prefHeight="203.0" prefWidth="236.0" style="-fx-background-radius: 10; -fx-background-color: white;" styleClass="drinkicon" stylesheets="@../buttonStyle.css">
               <effect>
                  <DropShadow spread="0.85" />
               </effect>
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="58.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <Button fx:id="coffee" mnemonicParsing="false" onAction="#CoffeeFun" prefHeight="203.0" prefWidth="236.0" style="-fx-background-radius: 10;" styleClass="cofficon" stylesheets="@../buttonStyle.css">
               <effect>
                  <DropShadow spread="0.85" />
               </effect>
               <font>
                  <Font name="Berlin Sans FB Demi Bold" size="55.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
